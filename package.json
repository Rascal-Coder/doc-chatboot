{
  "name": "doc-chatboot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(ts|js|tsx)\"",
    "format": "ras prettier-write",
    "commit": "ras git-commit",
    "cleanup": "ras cleanup",
    "update-pkg": "ras update-pkg",
    "prepare": "simple-git-hooks"
  },
  "license": "MIT",
  "dependencies": {
    "@rascal/utils": "workspace:*",
    "@tailwindcss/forms": "^0.5.7",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "langchain": "^0.2.1",
    "lodash-es": "^4.17.21",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^1.10.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "simple-git-hooks": "2.9.0",
    "@rascal/scripts": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/parser": "^7.10.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm ras git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm ras lint-staged"
  }
}
